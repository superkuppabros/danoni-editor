{
  "name": "danoni-editor",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^16.15.0",
    "npm": "^8.5.5"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext ts,vue --ignore-path .gitignore ./src/",
    "prettier": "prettier --check ./src/",
    "fix": "run-s fix:lint fix:prettier",
    "fix:lint": "eslint --fix --ext ts,vue --ignore-path .gitignore ./src/",
    "fix:prettier": "prettier --write ./src/",
    "test": "vitest run",
    "test:watch": "vitest -w"
  },
  "dependencies": {
    "konva": "^9.0.1",
    "uikit": "^3.14.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/path-browserify": "^1.0.0",
    "@types/source-map-support": "^0.5.4",
    "@types/uikit": "^3.3.2",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/eslint-config-typescript": "^10.0.0",
    "canvas": "^2.8.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "lodash": "^4.17.21",
    "mock-local-storage": "^1.1.22",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.16",
    "vitest": "^0.10.5",
    "vue-tsc": "^0.34.11"
  }
}
